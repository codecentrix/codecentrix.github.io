<template type="1" name="Hattrick" version="1.0.0" sodinstaller="1.0.0" categories="Login">
	<description>Automatically log on Hattrick site.</description>
	<ico name="hattrick.ico"></ico>
	<preinstall/>
	<sod>
		<![CDATA[
// FREE code from CODECENTRIX
// http://www.codecentrix.com/
// http://codecentrix.blogspot.com/

// Open a browser or use the browser in foreground and navigate to hattrick login page.
var core     = new ActiveXObject("Twebst.Core");
var browser = core.foregroundBrowser;

if (browser == null)
{
	// No browser in foreground.
	browser = core.StartBrowser("http://www.hattrick.org/");
}
else
{
	browser.Navigate("http://www.hattrick.org/");
}

// Find login fields.
var u = browser.FindElement("input text");
var p = browser.FindElement("input password");
var i = browser.FindElement("input image", "name=submit");

// Log on to site.
if ((u != null) && (p != null) && (i != null))
{
	u.InputText("UUUUUUUUUU");
	p.InputText("PPPPPPPPPP");
	i.Click();
}
]]>
	</sod>
	<postinstall/>
</template>
