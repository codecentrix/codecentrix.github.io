<template type="1" name="Softpedia forum" version="1.0.0" sodinstaller="1.0.0" categories="Login">
	<description>Automatically log on Softpedia forum.</description>
	<ico name="softpedia.ico"></ico>
	<preinstall/>
	<sod>
		<![CDATA[
// FREE code from CODECENTRIX
// http://www.codecentrix.com/
// http://codecentrix.blogspot.com/

// Open a browser or use the browser in foreground and navigate to softpedia forum login page.
var core    = new ActiveXObject("Twebst.Core");
var browser = core.foregroundBrowser;

if (browser == null)
{
	// No browser in foreground.
	browser = core.StartBrowser("http://forum.softpedia.com");
}
else
{
	browser.Navigate("http://forum.softpedia.com");
}

// Find login fields.
var u = browser.FindElement("input text",      "name=UserName");
var p = browser.FindElement("input password",  "name=PassWord");
var i = browser.FindAllElements("input image", "src=login-button.gif").item(1);

// Log on to site.
if ((u != null) && (p != null) && (i != null))
{
	u.InputText("UUUUUUUUUU");
	p.InputText("PPPPPPPPPP");
	i.Click();
}
]]>
	</sod>
	<postinstall/>
</template>
