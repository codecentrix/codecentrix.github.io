<template type="1" name="Twitter" version="1.0.0" sodinstaller="1.0.0" categories="Login">
	<description>Automatically log on Twitter site.</description>
	<ico name="twitter.ico"></ico>
	<preinstall/>
	<sod>
		<![CDATA[
// FREE code from CODECENTRIX
// http://www.codecentrix.com/
// http://codecentrix.blogspot.com/

// Open a browser or use the browser in foreground and navigate to twitter login page.
var core     = new ActiveXObject("Twebst.Core");
var browser = core.foregroundBrowser;

if (browser == null)
{
	// No browser in foreground.
	browser = core.StartBrowser("http://www.twitter.com/");
}
else
{
	browser.Navigate("http://www.twitter.com/");
}

// Find login fields.
var u = browser.FindElement("input text",     "id=username");
var p = browser.FindElement("input password", "id=password");
var s = browser.FindElement("input submit",   "id=signin_submit");

// Log on to site.
if ((u != null) && (p != null) && (s != null))
{
	u.InputText("UUUUUUUUUU");
	p.InputText("PPPPPPPPPP");
	s.Click();
}
]]>
	</sod>
	<postinstall/>
</template>
