<template type="1" name="Youtube" version="1.0.0" sodinstaller="1.0.0" categories="Login">
	<description>Automatically log on Youtube site.</description>
	<ico name="youtube.ico"></ico>
	<preinstall/>
	<sod>
		<![CDATA[
// FREE code from CODECENTRIX
// http://www.codecentrix.com/
// http://codecentrix.blogspot.com/

// Open a browser or use the browser in foreground and navigate to login page.
var core    = new ActiveXObject("Twebst.Core");
var browser = core.foregroundBrowser;

if (browser == null)
{
	// No browser in foreground.
	browser = core.StartBrowser("http://www.youtube.com/login?next=/");
}
else
{
	browser.Navigate("http://www.youtube.com/login?next=/");
}

// Find login fields.
var u = browser.FindElement("input text", "name=username");
var p = browser.FindElement("input password");
var a = browser.FindElement("a", "id=button-signin-link");

// Log on to site.
if ((u != null) && (p != null) && (a != null))
{
	u.InputText("UUUUUUUUUU");
	p.InputText("PPPPPPPPPP");
	a.Click();
}

]]>
	</sod>
	<postinstall/>
</template>
