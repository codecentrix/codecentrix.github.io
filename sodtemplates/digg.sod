<template type="1" name="Digg" version="1.0.0" sodinstaller="1.0.0" categories="Login">
	<description>Automatically log on Digg.com site.</description>
	<ico name="digg.ico"></ico>
	<preinstall/>
	<sod>
		<![CDATA[
// FREE code from CODECENTRIX
// http://www.codecentrix.com/
// http://codecentrix.blogspot.com/

// Open a browser or use the browser in foreground and navigate to login page.
var core     = new ActiveXObject("Twebst.Core");
var browser = core.foregroundBrowser;

if (browser == null)
{
	// No browser in foreground.
	browser = core.StartBrowser("http://www.digg.com/");
}
else
{
	browser.Navigate("http://www.digg.com/");
}

// Click on Login anchor.
var a = browser.FindElement("a", "text=Login");
if (a != null)
{
	a.Click();
}

// Find login fields.
var u = browser.FindElement("input text",     "id=side-username");
var p = browser.FindElement("input password", "id=side-password");
var b = browser.FindElement("button",         "name=submit");

// Log on to site.
if ((u != null) && (p != null) && (b != null))
{
	u.InputText("UUUUUUUUUU");
	p.InputText("PPPPPPPPPP");
	b.Click();
}
]]>
	</sod>
	<postinstall/>
</template>
