<template type="1" name="Hotmail" version="1.0.0" sodinstaller="1.0.0" categories="Login">
	<description>Automatically log on Hotmail site.</description>
	<ico name="microsoft.ico"></ico>
	<preinstall/>
	<sod>
		<![CDATA[
// FREE code from CODECENTRIX
// http://www.codecentrix.com/
// http://codecentrix.blogspot.com/

// Open a browser or use the browser in foreground and navigate to hotmail login page.
var core    = new ActiveXObject("Twebst.Core");
var browser = core.foregroundBrowser;

if (browser == null)
{
	// No browser in foreground.
	browser = core.StartBrowser("http://hotmail.com");
}
else
{
	browser.Navigate("http://hotmail.com");
}

// Find login fields.
var u = browser.FindElement("input text");
var p = browser.FindElement("input password");
var s = browser.FindElement("input submit");

// Log on to site.
if ((u != null) && (p != null) && (s != null))
{
	u.InputText("UUUUUUUUUU");
	p.InputText("PPPPPPPPPP");
	s.Click();
}
]]>
	</sod>
	<postinstall/>
</template>
