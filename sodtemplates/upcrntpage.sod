<template type="2" name="Current web page" version="1.0.0" sodinstaller="1.0.0" categories="Upload">
	<description>Upload files directly from desktop using the web page which you are currently working with.</description>
	<ico name="upcrnt.ico"></ico>
	<preinstall/>
	<sod>
		<![CDATA[
// FREE code from CODECENTRIX
// http://www.codecentrix.com/
// http://codecentrix.blogspot.com/

if (WScript.Arguments.length == 0)
{
	WScript.Echo("This is an upload script and it can not be launched directly!\n" +
	             "To use this script, right click on a file and choose from the context menu:\n" +
				 "'Script of the day' upload to");
	WScript.Quit(1);
}

// Find all borwsers.
var core = new ActiveXObject("Twebst.Core");
core.loadTimeout = 0;

var browsers         = core.FindAllBrowsers();
var browserToPutInFg = null;

// For each browser find the collection of <input type=file> controls.
for (i = 0; i < browsers.length; ++i)
{
	var brws    = browsers(i);
	var uploads = brws.FindAllElements("input file");

	for (j = 0; j < Math.min(uploads.length, WScript.Arguments.length); ++j)
	{
		var up = uploads(j);
		up.InputText(WScript.Arguments(j));

		if (browserToPutInFg == null)
		{
			browserToPutInFg = brws;
		}
	}
}

if (browserToPutInFg == null)
{
	WScript.Echo("No upload web page was found!");
}
else
{
	browserToPutInFg.ShowBrowserWindow(browserToPutInFg.SHOW_IN_FOREGROUND);
}
]]>
	</sod>
	<postinstall/>
</template>
